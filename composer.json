{
  "name": "vzina/yaf",
  "description": "The yaf Framework.",
  "keywords": [
    "framework",
    "yaf"
  ],
  "license": "MIT",
  "type": "yaf",
  "require": {
    "php": ">=5.4.0",
    "phpunit/phpunit": "4.8.18",
    "pagon/childprocess": "0.0.x",
    "pagon/eventemitter": "*",
    "pagon/logger": "*",
    "pagon/argparser": "*"
  },
  "require-dev": {
    "pagon/composer-packer": "*"
  },
  "autoload": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.phpcomposer.com"
    },
    {"packagist": false}
  ]
}